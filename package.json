{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Boiler Plate for React and Express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \" npm run client\""
  },
  "author": "Jacob Dymond",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.0.1",
    "create-react-app": "^2.0.4",
    "express": "^4.16.4",
    "nodemon": "^1.18.4"
  }
}
